<div class="row">
    <div class="col-md-12">

        <% if (typeof(error) !== "undefined") { %>
            <div class="alert alert-danger" role="alert">
                <strong>Error:</strong> <%= error %>
            </div>
        <% } else { %>
            <table class="table table-striped table-bordered" id="user_management">
                <thead>
                <tr>
                    <th>Brand ID</th>
                    <th>Brand Name</th>
                    <th class="text-center">Model Count</th>
                    <th><i class="fas fa-eye"></i></th>
                </tr>
                </thead>
                <tbody>
                <% brands.forEach(brand => { %>
                    <tr>
                        <td><%= brand._id %></td>
                        <td><%= brand.name%></td>
                        <td class="text-center"><%= brand.models.length %></td>
                        <td><a href='/admin/types/brands/<%= brand._id %>'>view</a></td>
                    </tr>
                <% }) %>

                </tbody>
            </table>
        <% } %>
    </div>
</div>
