<div class="row">
    <div class="col-md-12">

        <% if (typeof(error) !== "undefined") { %>
            <div class="alert alert-danger" role="alert">
                <strong>Error:</strong> <%= error %>
            </div>
        <% } else { %>
            <table class="table table-striped table-bordered" id="user_management">
                <thead>
                <tr>
                    <th>Model ID</th>
                    <th>Model Name</th>
                    <th>Brand</th>
                    <th>Category</th>
                    <th><i class="fas fa-eye"></i></th>
                </tr>
                </thead>
                <tbody>
                <% models.forEach(model => { %>
                    <tr>
                        <td><%= model._id %></td>
                        <td><%= model.name %></td>
                        <td><%= model.brand.name %></td>
                        <td><%= model.deviceType.name %></td>
                        <td><a href='/admin/types/models/<%= model._id %>'>view</a></td>
                    </tr>
                <% }) %>

                </tbody>
            </table>
        <% } %>
    </div>
</div>
